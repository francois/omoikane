<div class="small-12 columns">
  <div data-alert class="alert-box alert">
    <h5>Error(s) on this form prevented validation:</h5>
    <ul>
      <% form.errors.full_messages.each do |error| %>
        <li><%= error %></li>
      <% end %>
    </ul>
    <a href="#" class="close">&times;</a>
  </div>
</div>

